# =============================
# Bot config
# =============================

# Token bot Discord
BOT_TOKEN=your_discord_bot_token_here

# ID của bot (Application ID)
CLIENT_ID=your_discord_bot_client_id_here

# ID server mà bot hoạt động
GUILD_ID=your_discord_guild_id_here

# ID role được phép sử dụng bot
ALLOWED_ROLE_ID=your_discord_manager_role_here

# ID role sẽ được tag khi giao dịch thành công
CUSTOMER_ROLE_ID=1your_discord_customer_role_here

# ID của kênh dùng để ghi log giao dịch
LOG_CHANNEL_ID=your_discord_log_channel_here

# =============================
# Google Sheets config
# =============================

# ID bảng Google Sheet cần đọc
SHEET_ID=your_gooogle_sheet_id_here

# Dải dữ liệu cần quét (ví dụ: Sheet1!A2:C100)
# Đặt vị trí đầu và cuối
# Cột E là mã giao dịch
# Cột H là số tiền
SHEET_RANGE='Sheet1!E:H'

# Đường dẫn đến file service account credentials
SERVICE_ACCOUNT_FILE=./path/to/your/service_account

# =============================
# Prefix nội dung chuyển khoản
# =============================

# Prefix riêng cho từng ngân hàng
PREFIX_MBBANK=DST
PREFIX_TPBANK=TKPDST

# =============================
# Danh sách tài khoản ngân hàng (dạng JSON)
# =============================

# JSON chứa thông tin các ngân hàng hỗ trợ
BANK_ACCOUNTS='{
  "mbbank": {
    "name": "DST Studio",
    "accountNo": "123456789",
    "bankId": "970422",
    "displayName": "MB Bank"
  },
